{
  "name": "lisky",
  "version": "0.1.0",
  "description": "A command line interface for Lisk",
  "main": "index.js",
  "bin": {
    "lisky": "./bin/lisky"
  },
  "scripts": {
    "start": "node index.js",
    "test": "NODE_ENV=test nyc mocha",
    "test:watch": "npm test -- --watch",
    "test:now": "npm test -- --grep @now",
    "test:now:watch": "npm run test:now -- --watch",
    "cover": "NODE_ENV=test nyc --reporter=html --reporter=text mocha",
    "prepush": "grunt eslint && npm test",
    "postinstall": "npm link lisky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LiskHQ/lisky.git"
  },
  "keywords": [
    "lisk",
    "blockchain",
    "command-line",
    "nodejs",
    "javascript"
  ],
  "author": "Lisk Foundation <admin@lisk.io>, lightcurve GmbH <admin@lightcurve.io>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/LiskHQ/lisky/issues"
  },
  "homepage": "https://github.com/LiskHQ/lisky#readme",
  "devDependencies": {
    "eslint-config-airbnb-base": "=11.2.0",
    "eslint-config-lisk-base": "^0.1.0",
    "eslint-plugin-import": "=2.2.0",
    "grunt": "=1.0.1",
    "grunt-mocha-test": "=0.13.2",
    "gruntify-eslint": "=4.0.0",
    "husky": "=0.14.3",
    "mocha": "=3.3.0",
    "nyc": "=11.0.3",
    "should": "=11.2.1",
    "sinon": "=2.3.1"
  },
  "dependencies": {
    "cli-table2": "=0.2.0",
    "file-system": "=2.2.2",
    "fs-extra": "=3.0.1",
    "lisk-js": "=0.4.1",
    "vorpal": "LiskHQ/vorpal#master"
  }
}
