{
  	"extends": ["lisk-base"],
	"env": {
		"node": true
	},
  	"plugins": ["mocha"],
  	"rules": {
	  "no-param-reassign": 0,
	  "no-unused-expressions": 0,
	  "camelcase": 0,
	  "no-underscore-dangle": 0,
	  "new-cap": 0,
	  "max-len": 0,
	  "func-names": 0,
	  "no-use-before-define": 0,
	  "prefer-destructuring": 0,
	  "import/order": 0,
	  "no-shadow": 0,
	  "consistent-return": 0,
	  "no-plusplus": 0,
	  "global-require": 0,
	  "one-var": 0,
	  "radix": 0,
	  "no-template-curly-in-string": 0,
	  "vars-on-top": 0,
	  "comma-dangle": 0,
	  "strict": 0,
	  "dot-notation": 0,
	  "one-var-declaration-per-line": 0,
	  "import/no-dynamic-require": 0,
	  "no-new": 0,
	  "no-else-return": 0,
	  "lines-around-directive": 0,
	  "import/newline-after-import": 0,
	  "no-var": 0,
	  "object-shorthand": 0
	}
}
