{
  	"extends": ["lisk-base"],
	"env": {
		"node": true
	},
  	"plugins": ["mocha"],
  	"rules": {
	  "require-jsdoc": 0,
	  "no-param-reassign": 0,
	  "no-unused-expressions": 0,
	  "camelcase": 0,
	  "no-underscore-dangle": 0,
	  "new-cap": 0,
	  "max-len": 0,
	  "func-names": 0,
	  "no-use-before-define": 0,
	  "prefer-destructuring": 0,
	  "import/order": 0,
	  "no-shadow": 0,
	  "consistent-return": 0,
	  "no-plusplus": 0,
	  "global-require": 0,
	  "one-var": 0,
	  "radix": 0,
	  "no-template-curly-in-string": 0,
	  "vars-on-top": 0,
	  "comma-dangle": 0,
	  "strict": 0,
	  "dot-notation": 0,
	  "import/no-dynamic-require": 0,
	  "no-new": 0,
	  "no-else-return": 0,
	  "lines-around-directive": 0,
	  "import/newline-after-import": 0,
	  "no-var": 0,
	  "object-shorthand": 0,
	  "prefer-arrow-callback": 0,
	  "eqeqeq": 0,
	  "array-callback-return": 0,
	  "import/no-extraneous-dependencies": 0,
	  "no-console": 0,
	  "no-nested-ternary": 0,
	  "default-case": 0,
	  "no-loop-func": 0,
	  "arrow-parens": 0,
	  "newline-per-chained-call": 0,
	  "arrow-body-style": 0,
	  "no-restricted-globals": 0,
	  "prefer-rest-params": 0,
	  "no-bitwise": 0,
	  "no-restricted-properties": 0,
	  "no-useless-escape": 0,
	  "no-multi-assign": 0,
	  "no-prototype-builtins": 0,
	  "block-scoped-var": 0,
	  "no-restricted-syntax": 0,
	  "no-throw-literal": 0,
	  "guard-for-in": 0
	}
}
