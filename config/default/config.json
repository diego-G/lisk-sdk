{
	"components": {
		"logger": {
			"fileLogLevel": "info",
			"logFileName": "logs/lisk.log",
			"consoleLogLevel": "none"
		},
		"storage": {
			"host": "localhost",
			"port": 5432,
			"database": "",
			"user": "lisk",
			"password": "password",
			"min": 10,
			"max": 95,
			"poolIdleTimeout": 30000,
			"reapIntervalMillis": 1000,
			"logEvents": ["error"],
			"logFileName": "logs/lisk_db.log"
		},
		"cache": {
			"host": "127.0.0.1",
			"port": 6380,
			"db": 0,
			"password": null,
			"enabled": false
		}
	},
	"modules": {
		"http_api": {
			"enabled": true,
			"access": {
				"public": false,
				"whiteList": ["127.0.0.1"]
			},
			"ssl": {
				"enabled": false,
				"options": {
					"port": 443,
					"address": "0.0.0.0",
					"key": "./ssl/lisk.key",
					"cert": "./ssl/lisk.crt"
				}
			},
			"options": {
				"limits": {
					"max": 0,
					"delayMs": 0,
					"delayAfter": 0,
					"windowMs": 60000,
					"headersTimeout": 5000,
					"serverSetTimeout": 20000
				},
				"cors": {
					"origin": "*",
					"methods": ["GET", "POST", "PUT"]
				}
			},
			"trustProxy": false,
			"httpPort": 4000,
			"address": "0.0.0.0"
		},
		"chain": {
			"broadcasts": {
				"active": true,
				"broadcastInterval": 5000,
				"broadcastLimit": 25,
				"parallelLimit": 20,
				"releaseLimit": 25,
				"relayLimit": 3
			},
			"transactions": {
				"maxTransactionsPerQueue": 1000
			},
			"forging": {
				"force": false,
				"delegates": [],
				"access": {
					"whiteList": ["127.0.0.1"]
				}
			},
			"syncing": {
				"active": true
			},
			"loading": {
				"loadPerIteration": 5000
			},
			"network": {
				"enabled": true,
				"list": [
					{
						"ip": "testnet-seed-01.lisk.io",
						"wsPort": 7001
					},
					{
						"ip": "testnet-seed-02.lisk-nodes.net",
						"wsPort": 7001
					},
					{
						"ip": "testnet-seed-03.lisk.io",
						"wsPort": 7001
					},
					{
						"ip": "testnet-seed-04.lisk-nodes.net",
						"wsPort": 7001
					},
					{
						"ip": "testnet-seed-05.lisk.io",
						"wsPort": 7001
					}
				],
				"access": {
					"blackList": []
				},
				"options": {
					"timeout": 5000,
					"broadhashConsensusCalculationInterval": 5000,
					"wsEngine": "ws"
				},
				"wsPort": 5000,
				"address": "0.0.0.0"
			}
		}
	}
